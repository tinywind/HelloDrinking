<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	
	<ScrollView
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="8">
		<LinearLayout
			android:id="@+id/frame_linear_contents"
			android:layout_width="fill_parent"
			android:layout_height="fill_parent"
			android:orientation="vertical"/>
	</ScrollView>

	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:layout_weight="1">

		<LinearLayout
			android:id="@+id/frame_linear_posts"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_weight="1">
			<TextView
				android:text="List"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/frame_linear_map"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_weight="1">
			<TextView
				android:text="Map"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/frame_linear_ar"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_weight="1">
			<TextView
				android:text="AR"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>

		<LinearLayout
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:layout_width="wrap_content"
			android:layout_weight="1">
			<TextView
				android:text="Member"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>


	</LinearLayout>

</LinearLayout>



